<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--MÀU CHỮ [00][000000] = [ALPHA][RGB]-->
    <!-- Tham khảo: https://www.google.com/design/spec/style/color.html#color-color-palette -->

    <!-- Màu chính (Thường là: 500) -->
    <color name="app_primary">#607d8b</color>
    <!-- Màu tối, thường dùng cho thanh Status (pin, time) -->
    <!-- +200 so với màu chính (Thường là: 700) -->
    <color name="app_color_primary_dark">#455a64</color>

    <!-- màu nền -->
    <color name="app_window_background">#EEEEEE</color>

    <!--Màu background control chọn-->
    <color name="app_control_highlight_background">#20000000</color> <!--eee-->
    <color name="app_control_activated">#689f38</color> <!--700: nghịch màu-->

    <!--Control rỗng, chưa có dữ liệu-->
    <color name="app_control_none">#EEE</color>

    <!-- màu thành điều hướng (Home, back) (900) -->
    <color name="app_navigation_bar">#263238</color>

    <!-- màu nhấn (nổi bật), (dùng cho nút) -->
    <color name="app_color_accent">#8bc34a</color> <!--500: nghịch màu-->
    <!--viền nút-->
    <color name="app_color_accent_bright">#808bc34a</color>

</resources>